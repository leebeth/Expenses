<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tienda.a_shop">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AgregarProducto"
            android:label="@string/title_activity_agregar_producto"></activity>
        <activity
            android:name=".CarritoDeVenta"
            android:label="@string/title_activity_carrito_de_venta"></activity>
        <activity
            android:name=".ListActivity"
            android:label="@string/title_activity_list"></activity>
        <activity
            android:name=".EditarProducto"
            android:label="Editar Producto"></activity>
        <activity
            android:name=".HistorialVentasActivity"
            android:label="@string/title_activity_historial_ventas"></activity>
        <activity
            android:name=".AgregarItemGasto"
            android:label="@string/title_activity_agregar_item_gasto"></activity>
    </application>

</manifest>